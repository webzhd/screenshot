{
    "manifest_version": 2,
    "name": "截图",
    "description": "截图",
    "version": "1.0.0",
    "permissions": [
      "<all_urls>",
      "tabs", 
      "activeTab", 
      "storage", 
      "unlimitedStorage", 
      "notifications", 
      "clipboardWrite", 
      "downloads", 
      "cookies"
    ],
    "browser_action": {
      "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
  }